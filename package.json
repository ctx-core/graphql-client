{
	"name": "@ctx-core/graphql-client",
	"version": "7.0.89",
	"description": "ctx-core graphql-client",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-client#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-client/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-client/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"@ctx-core/graphql": "workspace:^9.0.29",
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.31",
		"@ctx-core/fetch": "workspace:^11.0.81",
		"@ctx-core/object": "workspace:^21.0.23"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.108",
		"@types/node": "^16.11.7",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}