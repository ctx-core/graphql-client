{
	"name": "@ctx-core/graphql-client",
	"version": "7.4.14",
	"description": "ctx-core graphql-client",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-client#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-client/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-client.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./gql",
		"./graphql_validation_errors_",
		"./ValidationError"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/dom": "^15.1.10",
		"@ctx-core/env": "^17.4.10",
		"@ctx-core/fetch-undici": "^3.1.14",
		"@ctx-core/graphql": "^9.1.10",
		"@ctx-core/object": "^30.4.2"
	},
	"devDependencies": {
		"@types/node": "^20.9.1",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.3",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}