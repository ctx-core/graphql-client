{
	"name": "@ctx-core/graphql-client",
	"version": "6.0.8",
	"description": "ctx-core graphql-client",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-client#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-client/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-client/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^10.0.4",
		"@ctx-core/fetch": "workspace:^10.0.8",
		"@ctx-core/graphql": "workspace:^8.0.5",
		"@ctx-core/object": "workspace:^20.0.4"
	},
	"devDependencies": {
		"@types/node": "^15.12.4",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}