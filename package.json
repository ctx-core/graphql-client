{
	"name": "@ctx-core/graphql-client",
	"version": "7.0.9",
	"description": "ctx-core graphql-client",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-client#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-client/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-client/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module/*, options*/)\nmodule.exports = require('./main.js')\nexport * from './index.js'\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.7",
		"@ctx-core/fetch": "workspace:^11.0.8",
		"@ctx-core/graphql": "workspace:^9.0.6",
		"@ctx-core/object": "workspace:^21.0.5",
		"esm": "^3.2.25"
	},
	"devDependencies": {
		"@types/node": "^16.3.2",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
