{
	"name": "@ctx-core/graphql-client",
	"version": "7.0.108",
	"description": "ctx-core graphql-client",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-client#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-client/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-client.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"@ctx-core/graphql": "workspace:^9.0.30",
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.43",
		"@ctx-core/fetch": "workspace:^11.0.96",
		"@ctx-core/object": "workspace:^22.0.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/node": "^16.11.13",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}